{
  "expo": {
    "name": "Memento Mori",
    "slug": "memento-phone",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Frame2.png",
    "scheme": "memento",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/Frame2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Frame3.png",
        "backgroundColor": "#000000"
      },
      "package": "com.itssure.mementophone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6eabb952-e309-42b6-8dd8-8f28469205c7"
      }
    }
  }
}
