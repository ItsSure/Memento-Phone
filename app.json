{
  "expo": {
    "name": "memento mori",
    "slug": "memento-phone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/skull.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/skull.png",
      "resizeMode": "contain",
      "backgroundColor": "#0000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.itssure.mementophone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6eabb952-e309-42b6-8dd8-8f28469205c7"
      }
    }
  }
}
